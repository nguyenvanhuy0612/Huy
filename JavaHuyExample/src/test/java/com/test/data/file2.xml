<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<suite name="Collect" parallel="false">
    <test name="verify user can create callback with system timezone and phone number timezone is different using Local Time" preserve-order="true">
        		
        <parameter name="dataFile" value="WorkspaceCCaaS.xlsx"/>
        		
        <parameter name="dataSheet" value="CallBack"/>
        		
        <classes>
            			
            <class name="com.scripts.UWFWorkSpacesCallback_Test">
                				
                <methods>
                    					
                    <include name="verifySystemAndPhoneNumTimezoneDifferentUseLocalTime"/>
                    				
                </methods>
                			
            </class>
            		
        </classes>
        	
    </test>
